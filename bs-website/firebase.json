{
  "hosting": [
    {
      "target": "en",
      "public": "dist/bs-website/en-US",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
